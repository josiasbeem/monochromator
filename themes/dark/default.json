    "name": "Monochromator Dark",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "string.quoted.docstring",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#9a9996"
            }
        },
        {
            "name": "Gray",
            "scope": [
                "keyword.operator",
                "punctuation",
                "keyword.control.ternary",
                "storage.type.function.arrow",
                "keyword.other.r",
                "keyword.control.newline.tex",
                "keyword.control.equation.newline.latex",
                "keyword.control.equation.align.latex",
                "keyword.control.table.cell.latex",
                "keyword.control.table.newline.latex",
                "storage.modifier.array"
            ],
            "settings": {
                "fontStyle": "normal",
                "foreground": "#9a9996"
            }
        },
        {
            "name": "Important gray",
            "scope": [
                "storage.type.annotation",
                "keyword.other.documentation",
                "storage.type.class.doxygen.cpp",
                "keyword.control.directive.include",
                "keyword.control.directive.define"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#9a9996"
            }
        },
        {
            "name": "Important",
            "scope": [
                "entity.name.function",
                "entity.name.type",
                "entity.other.inherited-class",
                "keyword",
                "storage.type",
                "support.class",
                "support.function",
                "support.type",
                "support.type.property-name.table.toml",
                "entity.name.section",
                "constant.other.math.tex",
                "constant.character.math.tex",
                "constant.other.general.math.tex",
                "entity.name.tag",
                "entity.other.attribute-name.class.css"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Normal",
            "scope": [
                "support.type.property-name",
                "entity.name.tag.yaml",
                "support.class.latex",
                "entity.name.function.preprocessor",
                "variable.other",
                "variable.parameter.positional.shell",
                "support.type.vendored.property-name",
                "storage.modifier.import",
                "support.class.math.block.tex",
                "support.class.math.block.environment.latex",
                "constant.other.caps.rust",
                "meta.interpolation.rust",
                "constant.other.enum",
                "constant.other.placeholder",
                "string.quoted.other.lt-gt.include",
                "storage.modifier.package"
            ],
            "settings": {
                "fontStyle": "normal",
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Literals",
            "scope": [
                "constant.numeric",
                "constant.character",
                "string",
                "keyword.other.unit",
                "constant.other"
            ],
            "settings": {
                "fontStyle": "normal",
                "foreground": "#3584e4"
            }
        },
        {
            "name": "Bold literals",
            "scope": [
                "constant.language",
                "constant.character.escape"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#3584e4"
            }
        },
        {
            "name": "Constants",
            "scope": "constant",
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffffff"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#ffffff"
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#3584e4"
            }
        },
        {
            "scope": "markup.quote",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#9a9996"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "markup.strikethrough",
            "settings": {
                "fontStyle": "strikethrough"
            }
        }
    ]
}
