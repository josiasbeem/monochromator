{
    "name": "Monochromator Dark",
    "colors": {
        "activityBar.background": "#303030",
        "activityBar.border": "#444444",
        "activityBar.foreground": "#ffffff",
        "activityBar.inactiveForeground": "#bfbfbf",
        "cursor.foreground": "#ffffff",
        "editor.background": "#1e1e1e",
        "editor.foreground": "#ffffff",
        "editorLineNumber.activeForeground": "#ffffff",
        "editorLineNumber.foreground": "#bfbfbf",
        "focusBorder": "#444444",
        "list.activeSelectionBackground": "#444444",
        "list.activeSelectionForeground": "#ffffff",
        "list.hoverBackground": "#444444",
        "list.hoverForeground": "#ffffff",
        "list.inactiveSelectionBackground": "#444444",
        "list.inactiveSelectionForeground": "#ffffff",
        "sideBar.background": "#303030",
        "sideBar.border": "#444444",
        "sideBar.foreground": "#ffffff",
        "sideBarSectionHeader.background": "#303030",
        "sideBarSectionHeader.border": "#444444",
        "sideBarSectionHeader.foreground": "#ffffff",
        "sideBarTitle.foreground": "#ffffff",
        "statusBar.background": "#303030",
        "statusBar.border": "#444444",
        "statusBar.foreground": "#ffffff",
        "statusBarItem.hoverBackground": "#444444",
        "statusBarItem.remoteBackground": "#303030",
        "statusBarItem.remoteForeground": "#ffffff",
        "statusBar.noFolderBackground": "#303030",
        "titleBar.activeBackground": "#303030",
        "titleBar.activeForeground": "#ffffff",
        "titleBar.border": "#444444",
        "titleBar.inactiveBackground": "#303030",
        "titleBar.inactiveForeground": "#bfbfbf"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#bfbfbf"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator",
                "punctuation"
            ],
            "settings": {
                "fontStyle": "normal",
                "foreground": "#bfbfbf"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "ffffff"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Sections",
            "scope": "entity.name.section",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Numbers, Characters",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant"
            ],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#bfbfbf"
            }
        }
    ]
}
