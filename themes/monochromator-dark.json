    "name": "Monochromator Dark",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#9a9996"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator",
                "punctuation"
            ],
            "settings": {
                "fontStyle": "normal",
                "foreground": "#9a9996"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Storage types",
            "scope": [
                "storage.type"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Support types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Sections",
            "scope": "entity.name.section",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Literals",
            "scope": [
                "constant.numeric",
                "constant.character"
            ],
            "settings": {
                "fontStyle": "normal",
                "foreground": "#3584e4"
            }
        },
        {
            "name": "Booleans",
            "scope": "constant.language",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#3584e4"
            }
        },
        {
            "name": "Literal constants",
            "scope": "constant",
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#3584e4"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffffff"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#ffffff"
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#3584e4"
            }
        },
        {
            "scope": "markup.quote",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#9a9996"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        }
    ]
}
