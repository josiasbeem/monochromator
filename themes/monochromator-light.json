    "name": "Monochromator Light",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#77767b"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator",
                "punctuation"
            ],
            "settings": {
                "fontStyle": "normal",
                "foreground": "#77767b"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#000000"
            }
        },
        {
            "name": "Storage types",
            "scope": [
                "storage.type"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#000000"
            }
        },
        {
            "name": "Support types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#000000"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#000000"
            }
        },
        {
            "name": "Sections",
            "scope": "entity.name.section",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#000000"
            }
        },
        {
            "name": "Literals",
            "scope": [
                "constant.numeric",
                "constant.character"
            ],
            "settings": {
                "fontStyle": "normal",
                "foreground": "#3584e4"
            }
        },
        {
            "name": "Booleans",
            "scope": "constant.language",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#3584e4"
            }
        },
        {
            "name": "Literal constants",
            "scope": "constant",
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#3584e4"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#000000"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#000000"
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#3584e4"
            }
        },
        {
            "scope": "markup.quote",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#77767b"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        }
    ]
}
