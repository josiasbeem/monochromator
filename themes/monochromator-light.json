        "titleBar.border": "#d9d9d9",
        "titleBar.inactiveBackground": "#ebebeb"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#77767b"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator",
                "punctuation"
            ],
            "settings": {
                "fontStyle": "normal",
                "foreground": "#77767b"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#000000"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#000000"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "000000"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#000000"
            }
        },
        {
            "name": "Sections",
            "scope": "entity.name.section",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#000000"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#a347ba"
            }
        },
        {
            "name": "Numbers, Characters",
            "scope": [
                "constant.numeric",
                "constant.character"
            ],
            "settings": {
                "fontStyle": "normal",
                "foreground": "#a347ba"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#a347ba"
            }
        }
    ]
}
